package com.cook.api.model

import com.dix.base.db.model.BaseModel
import com.dix.base.db.model.Column
import com.dix.base.db.model.Model
import com.dix.base.db.model.ModelJsonSerializer
import com.fasterxml.jackson.databind.annotation.JsonSerialize

/**
*  <%= tableComment %>
*  created by whis
*/
@Model(tableName = "<%= tableName %>")
@JsonSerialize(using = ModelJsonSerializer::class)
class <%= modelName %> : BaseModel() {

    <% fieldList.forEach(function (item) { %>@Column("<%= item['jsonName'] %>") var <%= item['modelName'] %>: <%= item['dataType'] %> = <%- item['dataTypeValue'] %> <% if (item['comment']) { %> // <%= item["comment"] %> <% } %>
    <% }); %>

    companion object {
        val keys = arrayOf(<%- fieldString %>)
    }
}
