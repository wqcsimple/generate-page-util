package com.lear.client.mapper

import com.fly.base.mybatis.sqlprovider.SqlProvider
import org.apache.ibatis.annotations.Mapper
import org.springframework.stereotype.Component
import org.apache.ibatis.annotations.Param
import org.apache.ibatis.annotations.SelectProvider

@Mapper
@Component
interface <%= modelName %>Mapper {

    @SelectProvider(type = SqlProvider::class, method = "select")
    fun select(@Param("condition") condition: Any, @Param("otherCondition") otherCondition: String?): List<<%= modelName %>>
}
